archetype (adl_version=1.4)
	openEHR-EHR-EVALUATION.goal.v1

concept
	[at0000]	-- Goal
language
	original_language = <[ISO_639-1::en]>
	translations = <
		["ar-sy"] = <
			language = <[ISO_639-1::ar-sy]>
