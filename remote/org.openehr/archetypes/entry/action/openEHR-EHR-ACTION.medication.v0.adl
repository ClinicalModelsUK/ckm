		description matches {
			ITEM_TREE[at0017] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- Medication
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0082] occurrences matches {0..*} matches {	-- Product
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0104] occurrences matches {0..1} matches {	-- Preparation details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication_substance(-[a-zA-Z0-9_]+)*\.v0/}
							}
							ELEMENT[at0130] occurrences matches {0..1} matches {	-- Manufacturer
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0083] occurrences matches {0..1} matches {	-- Batch ID
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0084] occurrences matches {0..1} matches {	-- Expiry date
								value matches {
									DV_DATE matches {*}
								}
							}
							allow_archetype CLUSTER[at0131] occurrences matches {0..1} matches {	-- Amount
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication_supply_amount(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.dosage_administration(-[a-zA-Z0-9_]+)*\.v0/}
							}
							ELEMENT[at0105] occurrences matches {0..1} matches {	-- Max class dose administered
								value matches {
									DV_PROPORTION matches {
										numerator matches {|0.0..100.0|}
										is_integral matches {False}
										type matches {2}
									}
								}
							}
							ELEMENT[at0132] occurrences matches {0..1} matches {	-- Dispensing substitution
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0138, 	-- Substitute product dispensed
											at0139]	-- Substitute product not dispensed
										}
									}
								}
							}
							ELEMENT[at0133] occurrences matches {0..1} matches {	-- Dispensing substitution reason
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0043] occurrences matches {0..1} matches {	-- Order scheduled datetime
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0021] occurrences matches {0..*} matches {	-- Reason
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0140] occurrences matches {0..1} matches {	-- Delivery details
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0141] occurrences matches {0..1} matches {	-- Body site
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0142] occurrences matches {0..1} matches {	-- Structured body site
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_clock(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0143] occurrences matches {0..*} matches {	-- Delivery method
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0144] occurrences matches {0..*} matches {	-- Delivery device details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					allow_archetype CLUSTER[at0053] occurrences matches {0..*} matches {	-- Details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication_authorisation(-[a-zA-Z0-9_]+)*\.v0/}
					}
					ELEMENT[at0033] occurrences matches {0..*} matches {	-- Patient guidance
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- Sequence number
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0030] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0103] occurrences matches {0..*} matches {	-- Order ID
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					allow_archetype CLUSTER[at0085] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}



ontology
	term_definitions = <
		["ar-sy"] = <
			items = <
				["at0000"] = <
					text = <"*Medication(en)">
					description = <"*Details of use, administration, dispensing or other care step relating to a medicine, vaccine or other therapeutic good which may arise from an instruction from a clinician.(en)">
				>
				["at0002"] = <
					text = <"*Prescription first issued(en)">
					description = <"*The instruction has issued as an item on a prescription.(en)">
				>
				["at0003"] = <
					text = <"*Medication dispensed(en)">
					description = <"*The medication has been dispensed.(en)">
				>
				["at0004"] = <
					text = <"*Medication course commenced(en)">
					description = <"*The medication has been taken by, or administered to, the patient for the first time.(en)">
				>
				["at0005"] = <
					text = <"*Medication course reviewed(en)">
					description = <"*The medication has been reviewed.(en)">
				>
				["at0006"] = <
					text = <"*Dose administered(en)">
					description = <"*A single administration of the medication has taken place.(en)">
				>
				["at0007"] = <
					text = <"*Medication course completed(en)">
					description = <"*The medication course has been completed as planned.(en)">
				>
				["at0008"] = <
					text = <"*Medication course supply delayed(en)">
					description = <"*The medication course has not been dispensed as supply is not yet available.(en)">
				>
				["at0009"] = <
					text = <"*Medication course suspended(en)">
					description = <"*The administration of the medication has been suspended.(en)">
				>
				["at0010"] = <
					text = <"*Prescription re-issued(en)">
					description = <"*A prescription token has been re-issued for an existing medication order.(en)">
				>
				["at0011"] = <
					text = <"*Prescription re-issue suspended(en)">
					description = <"*Reissuing of this prescription is not available without some intervention.(en)">
				>
				["at0012"] = <
					text = <"*Medication course cancelled(en)">
					description = <"*The planned administration has been cancelled prior to any administration.(en)">
				>
				["at0013"] = <
					text = <"*Scheduled medication postponed(en)">
					description = <"*A scheduled medication has been postponed prior to any administration.(en)">
				>
				["at0015"] = <
					text = <"*Medication course stopped(en)">
					description = <"*The administration of this medication has been ceased before anticipated completion.(en)">
				>
				["at0016"] = <
					text = <"*Medication start date/condition set(en)">
					description = <"*The time to start this medication, or other starting condition, has been set.(en)">
				>
				["at0017"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0018"] = <
					text = <"*Dose administration omitted(en)">
					description = <"*The administration of a medication has been withheld and not given. There is no expectation that it will be given later, though the next dose (if any) should be administered according to the original order.(en)">
				>
				["at0020"] = <
					text = <"*Medication(en)">
					description = <"*The medicine, vaccine or other therapeutic good which was the focus of the action. This element will normally be coded with a medicines terminology but free text may be required in some cases.(en)">
				>
				["at0021"] = <
					text = <"*Reason(en)">
					description = <"*The reason(s) the specific action or step was carried out. Note: This is not the reason for the medication instruction, rather the specific reason e.g. for administration.(en)">
					comment = <"*For example: 'Postponed - Patient not avalable at administration time', 'Cancelled - Adverse reaction'(en)">
				>
				["at0024"] = <
					text = <"*Comment(en)">
					description = <"*A comment on the action taken including details of any variance between the intended action and the action actually performed.(en)">
					comment = <"*For example: 'Patient was in radiology department'.(en)">
				>
				["at0025"] = <
					text = <"*Sequence number(en)">
					description = <"*The sequence number specific to the action being recorded.(en)">
				>
				["at0030"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0033"] = <
					text = <"*Patient guidance(en)">
					description = <"*Any guidance, instructions or advice given to the subject of care or carer at the time of the action.(en)">
				>
				["at0035"] = <
					text = <"*Dispensed medication returned.(en)">
					description = <"*A dispensed medication is returned, or not collected, without being used.(en)">
				>
				["at0039"] = <
					text = <"*Major change to order.(en)">
					description = <"*A major change to the order was required, resulting in this order being stopped and a replacement order being started.(en)">
				>
				["at0041"] = <
					text = <"*Minor change to order(en)">
					description = <"*The medication order has been changed in a manner which does not require a new instruction/order to be issued, according to local clinical rules.(en)">
				>
				["at0043"] = <
					text = <"*Order scheduled datetime(en)">
					description = <"*The datetime at which the medication order was scheduled to occur.(en)">
					comment = <"* Used to compare variance from actual action time where this is not readily calculable from the original instruction.(en)">
				>
				["at0044"] = <
					text = <"*Dose administration deferred(en)">
					description = <"*Administration of a dose of mediciation has been delayed but is expected to be given as soon as possible.(en)">
				>
				["at0053"] = <
					text = <"*Details(en)">
					description = <"*Further structured details of the action, possibly specific to a pathway step.(en)">
				>
				["at0082"] = <
					text = <"*Product(en)">
					description = <"*Details of the product dispensed, authorised or administered.(en)">
				>
				["at0083"] = <
					text = <"*New element(en)">
					description = <"**(en)">
				>
				["at0084"] = <
					text = <"*Expiry date(en)">
					description = <"*The expiry date of the medicine administered as documented by the manufacturer.(en)">
					comment = <"*For example: '2017-05-23'(en)">
				>
				["at0085"] = <
					text = <"*Cluster(en)">
					description = <"**(en)">
				>
				["at0103"] = <
					text = <"*Order ID(en)">
					description = <"*The identifier assigned with the original order. (en)">
				>
				["at0104"] = <
					text = <"*Preparation details(en)">
					description = <"*Details of the medication preparation. (en)">
				>
				["at0105"] = <
					text = <"*Max class dose administered(en)">
					description = <"*The proportion of the maximum allowed dose equivalent administered.(en)">
					comment = <"*Used in mental health to allow monitoring of total amount of a drug class administered e.g. anti-psychotics, where each medication is associated with a percentage contribution towards a total daily maximum, normally 100%.(en)">
				>
				["at0106"] = <
					text = <"*Medication re-authorised(en)">
					description = <"*A repeat prescription or dispense has been re-authorised.(en)">
				>
				["at0109"] = <
					text = <"*Medication recommended(en)">
					description = <"*The medication has been recommended but no steps have been taken to initiate prescribing.(en)">
				>
				["at0130"] = <
					text = <"*Manufacturer(en)">
					description = <"*The manufacturer of the medication.(en)">
					comment = <"*For example: 'Abbott'(en)">
				>
				["at0131"] = <
					text = <"*Amount(en)">
					description = <"*Details of the amount of medication supplied i.e. authorised, dispensed or administered.(en)">
					comment = <"*Dosage_administration is intended to carry details of medication order changes or dose administrations, whilst medication_supply_amount is intended for details of dispensing-related activity.(en)">
				>
				["at0132"] = <
					text = <"*Dispensing substitution(en)">
					description = <"*Subsitution action taken by the dispenser.(en)">
				>
				["at0133"] = <
					text = <"*Dispensing substitution reason(en)">
					description = <"*The reason or justification for the substitution action taken.(en)">
					comment = <"*For example: 'Generic alternaives are not bio-equivalent'.(en)">
				>
				["at0137"] = <
					text = <"*Medication restart date/condition set(en)">
					description = <"*A  suspended medication has had its restart date or condition set.(en)">
				>
				["at0138"] = <
					text = <"*Substitute product dispensed(en)">
					description = <"*A medication was dispensed which is bioequivalent to that ordered.(en)">
				>
				["at0139"] = <
					text = <"*Substitute product not dispensed(en)">
					description = <"*Although allowed by the medication order a substitute bioequvalent medication was not dispensed.(en)">
				>
				["at0140"] = <
					text = <"*Delivery details(en)">
					description = <"*Details of body site and delivery of the medication.(en)">
				>
